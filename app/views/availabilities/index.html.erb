<p id="notice"><%= notice %></p>

<h1>Listing Availabilities</h1>

<table>
  <thead>
    <tr>
      <th>Product</th>
      <th>Branch</th>
      <th>Stock</th>
      <th>Aisleno</th>
      <th>Sectionno</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @availabilities.each do |availability| %>
      <tr>
        <td><%= availability.product_id %></td>
        <td><%= availability.branch_id %></td>
        <td><%= availability.stock %></td>
        <td><%= availability.aisleno %></td>
        <td><%= availability.sectionno %></td>
        <td><%= link_to 'Show', availability %></td>
        <td><%= link_to 'Edit', edit_availability_path(availability) %></td>
        <td><%= link_to 'Destroy', availability, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Availability', new_availability_path %>
